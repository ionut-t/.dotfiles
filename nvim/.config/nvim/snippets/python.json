{
  "Function with Docstring": {
    "prefix": "def",
    "body": [
      "def ${1:function_name}(${2:params}) -> ${3:None}:",
      "    \"\"\"${4:Description}",
      "",
      "    Args:",
      "        ${5:param}: ${6:description}",
      "",
      "    Returns:",
      "        ${7:description}",
      "    \"\"\"",
      "    $0"
    ],
    "description": "Function with docstring"
  },
  "Class with Init": {
    "prefix": "cls",
    "body": [
      "class ${1:ClassName}:",
      "    \"\"\"${2:Class description}\"\"\"",
      "",
      "    def __init__(self, ${3:params}) -> None:",
      "        \"\"\"Initialize ${1:ClassName}.",
      "",
      "        Args:",
      "            ${4:param}: ${5:description}",
      "        \"\"\"",
      "        ${6:self.${3:params} = ${3:params}}",
      "        $0"
    ],
    "description": "Class with __init__"
  },
  "Pytest Test Function": {
    "prefix": "pytest",
    "body": [
      "def test_${1:function_name}():",
      "    \"\"\"Test ${2:description}.\"\"\"",
      "    # Arrange",
      "    ${3:pass}",
      "",
      "    # Act",
      "    ${4:result = None}",
      "",
      "    # Assert",
      "    assert ${5:result == expected}",
      "    $0"
    ],
    "description": "pytest test function"
  },
  "Pytest Fixture": {
    "prefix": "fixture",
    "body": [
      "@pytest.fixture",
      "def ${1:fixture_name}():",
      "    \"\"\"${2:Fixture description}.\"\"\"",
      "    ${3:value} = ${4:None}",
      "    yield ${3:value}",
      "    $0"
    ],
    "description": "pytest fixture"
  },
  "Async Function": {
    "prefix": "async",
    "body": [
      "async def ${1:function_name}(${2:params}) -> ${3:None}:",
      "    \"\"\"${4:Description}\"\"\"",
      "    $0"
    ],
    "description": "async function"
  },
  "Main Guard": {
    "prefix": "main",
    "body": [
      "if __name__ == \"__main__\":",
      "    ${1:main()}",
      "    $0"
    ],
    "description": "if __name__ == '__main__' guard"
  },
  "Dataclass": {
    "prefix": "dataclass",
    "body": [
      "@dataclass",
      "class ${1:ClassName}:",
      "    \"\"\"${2:Class description}\"\"\"",
      "",
      "    ${3:field}: ${4:str}",
      "    $0"
    ],
    "description": "Dataclass"
  },
  "Try Except": {
    "prefix": "tryexc",
    "body": [
      "try:",
      "    $0",
      "except ${1:Exception} as ${2:e}:",
      "    ${3:raise}"
    ],
    "description": "Try except block"
  },
  "Context Manager": {
    "prefix": "withopen",
    "body": [
      "with open(\"${1:filename}\", \"${2:r}\") as ${3:f}:",
      "    ${4:content} = ${3:f}.read()",
      "    $0"
    ],
    "description": "With open file"
  },
  "List Comprehension": {
    "prefix": "listcomp",
    "body": [
      "[${1:x} for ${1:x} in ${2:iterable}${3: if ${4:condition}}]"
    ],
    "description": "List comprehension"
  },
  "Dict Comprehension": {
    "prefix": "dictcomp",
    "body": [
      "{${1:key}: ${2:value} for ${3:item} in ${4:iterable}}"
    ],
    "description": "Dict comprehension"
  },
  "Logger Setup": {
    "prefix": "logger",
    "body": [
      "import logging",
      "",
      "logger = logging.getLogger(__name__)",
      "$0"
    ],
    "description": "Logger setup"
  },
  "Property": {
    "prefix": "property",
    "body": [
      "@property",
      "def ${1:name}(self) -> ${2:str}:",
      "    \"\"\"${3:Property description}.\"\"\"",
      "    return self._${1:name}",
      "",
      "@${1:name}.setter",
      "def ${1:name}(self, value: ${2:str}) -> None:",
      "    self._${1:name} = value",
      "    $0"
    ],
    "description": "Property with getter and setter"
  }
}
